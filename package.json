{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "type": "module",
  "dependencies": {
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-customers-bookshop": "cds watch --open com.alfa.customersbookshop/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "[development]": {
      "requires": {
        "auth": {
          "kind": "dummy"
        },
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": "test/db/db.sqlite"
          }
        }
      }
    }
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/customers-bookshop"
  ]
}
