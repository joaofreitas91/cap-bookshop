{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "type": "module",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "[development]": {
        "auth": {
          "kind": "mocked",
          "users": {
            "john": {
              "password": "1234",
              "roles": [
                "manager"
              ]
            },
            "bob": {
              "password": "1234",
              "roles": [
                "seller"
              ]
            }
          }
        },
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": "test/db/db.sqlite"
          }
        }
      },
      "[production]": {
        "auth": {
          "kind": "xsuaa"
        },
        "db": {
          "kind": "hana"
        }
      }
    }
  },
  "private": true
}