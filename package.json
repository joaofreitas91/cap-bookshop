{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "type": "module",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "express": "^4",
    "@sap/xssec": "^4",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3",
    "@sap-cloud-sdk/resilience": "^3"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "[development]": {
      "requires": {
        "auth": {
          "kind": "mocked",
          "users": {
            "john": {
              "password": "1234",
              "roles": [
                "manager"
              ]
            },
            "bob": {
              "password": "1234",
              "roles": [
                "seller"
              ]
            }
          }
        },
        "BusinessPartner": {
          "kind": "odata-v2",
          "model": "srv/external/BusinessPartner",
          "credentials": {
            "url": "https://sandbox.api.sap.com/s4hanacloud/sap/opu/odata/sap/API_BUSINESS_PARTNER",
            "headers": {
              "APIKey": "SDNpQ7QNVcaNqQ9yup5NTluvCA9YWeL7"
            }
          },
          "csrf": true,
          "csrfInBatch": true
        },
        "db": {
          "kind": "sqlite",
          "credentials": {
            "url": "test/db/db.sqlite"
          }
        }
      }
    },
    "[production]": {
      "requires": {
        "auth": {
          "kind": "xsuaa"
        },
        "BusinessPartner": {
          "kind": "odata-v2",
          "model": "srv/external/BusinessPartner",
          "credentials": {
            "path": "https://sandbox.api.sap.com/s4hanacloud/sap/opu/odata/sap/API_BUSINESS_PARTNER"
          },
          "csrf": true,
          "csrfInBatch": true
        },
        "db": {
          "kind": "hana"
        }
      }
    },
    "requires": {
      "connectivity": true,
      "businesspartner": {
        "kind": "odata-v2",
        "model": "srv/external/businesspartner"
      }
    }
  },
  "private": true
}